{
  "services": {
    "main": {
      "in": {
        "type": "number"
      },
      "out": {
        "type": "number"
      }
    }
  },
  "operators": {
    "childOpr": {
      "operator": "test_data.nested_op.sub.customOpDouble"
    }
  },
  "connections": {
    "(": [
      "(childOpr"
    ],
    "childOpr)": [
      ")"
    ]
  },
  "testCases": [
    {
      "name": "NumbersDouble",
      "description": "Numbers must be doubled",
      "data": {
        "in": [
          1,
          7.5,
          0
        ],
        "out": [
          2,
          15,
          0
        ]
      }
    },
    {
      "name": "StringsDouble",
      "description": "Strings must be doubled",
      "data": {
        "in": [
          "hello",
          "slang"
        ],
        "out": [
          "hellohello",
          "slangslang"
        ]
      }
    }
  ]
}